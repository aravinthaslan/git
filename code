1.
// #include <stdio.h>

void main(){
    int i,temp,a[20],j,n;
    scanf("%d",&n);
    temp=n;
    i=0;
    while(temp){
        a[i]=temp%6;
        temp=temp/6;
        i++;
    }
    for(j=i-1;j>=0;j--){
        printf("%d",a[i]);
    }
}

2.// Online C compiler to run C program online
#include <stdio.h>

void main() {
    int i,n,x,k;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    scanf("%d",&k);
    for(i=0;i<n;i++){
        if(a[i]<k)
        break;
    }
    if(i<n)
    printf("no");
    else
    printf("yes");
}

